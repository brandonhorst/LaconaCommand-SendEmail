{
  "name": "lacona-communicate",
  "version": "0.4.0",
  "description": "Lacona Command for making calls, sending facetimes, starting emails, and messages",
  "main": "build/extensions.js",
  "lacona": {
    "extensions": "build/extensions.js"
  },
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p build; browserify src/extensions.jsx -o build/extensions.js -x lacona-phrases -x elliptical -x lacona-api --standalone extensions --extension=jsx",
    "clean": "rimraf build",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-communicate.git"
  },
  "keywords": [
    "lacona",
    "command",
    "communicate",
    "phone",
    "text",
    "facetime",
    "email"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-communicate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-communicate",
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.8.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.1",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2",
    "lodash": "^4.12.0"
  },
  "dependencies": {},
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "browserify": {
    "transform": "babelify"
  }
}
