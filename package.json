{
  "name": "lacona-communicate",
  "version": "0.5.0",
  "description": "Lacona Command for making calls, sending facetimes, starting emails, and messages",
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-communicate.git"
  },
  "keywords": [
    "lacona",
    "command",
    "communicate",
    "phone",
    "text",
    "facetime",
    "email"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-communicate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-communicate",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc14",
    "lacona-api": "^0.6.2",
    "lacona-phrases": "^0.7.0",
    "lodash": "^4.13.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
