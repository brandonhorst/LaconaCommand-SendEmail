{
  "name": "lacona-communicate",
  "version": "0.0.1",
  "description": "Lacona Command for making calls, sending facetimes, starting emails, and messages",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers jsx:babel-register --watch-extensions jsx",
    "build": "mkdir -p build; babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonhorst/lacona-communicate.git"
  },
  "keywords": [
    "lacona",
    "command",
    "communicate",
    "phone",
    "text",
    "facetime",
    "email"
  ],
  "author": "@brandonhorst",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brandonhorst/lacona-communicate/issues"
  },
  "homepage": "https://github.com/brandonhorst/lacona-communicate",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-plugin-transform-react-jsx": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "elliptical": "^1.0.0-rc9",
    "elliptical-email": "^1.0.0-rc2",
    "elliptical-phone": "^1.0.0-rc2",
    "elliptical-string": "^1.0.0-rc2",
    "lacona-api": "0.1.0",
    "lacona-command": "^0.2.0",
    "lodash": "^4.9.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
